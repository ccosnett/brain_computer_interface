Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"tmpzip", "=", 
      RowBox[{"FileNameJoin", "[", 
       RowBox[{"{", 
        RowBox[{"$TemporaryDirectory", ",", "\"\<Shortcuts.zip\>\""}], "}"}], 
       "]"}]}], ",", 
     RowBox[{
     "gitzip", "=", 
      "\"\<https://raw.githubusercontent.com/rolfmertig/Shortcuts/master/\
Shortcuts.zip\>\""}]}], "}"}], ",", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"FileExistsQ", "[", "tmpzip", "]"}], ",", 
      RowBox[{"DeleteFile", "[", "tmpzip", "]"}]}], "]"}], ";", "\n", 
    RowBox[{"URLSave", "[", 
     RowBox[{"gitzip", ",", "tmpzip"}], "]"}], ";", "\n", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"!", 
       RowBox[{"FileExistsQ", "[", "tmpzip", "]"}]}], ",", 
      RowBox[{"Print", "[", "\"\<Installation failed!\>\"", "]"}], ",", 
      RowBox[{
       RowBox[{"Quiet", "@", 
        RowBox[{"DeleteDirectory", "[", 
         RowBox[{
          RowBox[{"FileNameJoin", "[", 
           RowBox[{"{", 
            RowBox[{
            "$UserBaseDirectory", ",", "\"\<Applications\>\"", ",", 
             "\"\<Shortcuts\>\""}], "}"}], "]"}], ",", 
          RowBox[{"DeleteContents", "\[Rule]", "True"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ExtractArchive", "[", 
        RowBox[{"tmpzip", ",", 
         RowBox[{"FileNameJoin", "[", 
          RowBox[{"{", 
           RowBox[{"$UserBaseDirectory", ",", "\"\<Applications\>\""}], "}"}],
